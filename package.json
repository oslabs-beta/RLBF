{
  "name": "rpsview",
  "version": "1.0.0",
  "private": true,
  "description": "RPS View is a new developer tool to visualize redis pub sub relationships in application development.",
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run electron\"",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack serve\" \"nodemon main.js\"",
    "test": "jest --verbose --detectOpenHandles",
    "electron": "electron .",
    "package-mac": "electron-packager . RPS-View --overwrite --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . RPS-View --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"RPS-View\"",
    "package-linux": "electron-packager . RPS-View --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "prod-electron": "NODE_ENV=production electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/RPS-View"
  },
  "author": "RLBF",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/RPS-View/issues"
  },
  "homepage": "https://github.com/oslabs-beta/RPS-View#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "electron": "^11.0.4",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.2.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "nodemon": "^2.0.6",
    "redis-mock": "^0.55.0",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^10.1.0",
    "serve": "^11.3.2",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "babel-jest": "^26.6.3",
    "body-parser": "^1.19.0",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "ioredis": "^4.19.2",
    "mongodb": "^3.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "^4.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.29.0",
    "wait-on": "^5.2.0",
    "ws": "^7.4.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "build": {
    "files": [
      {
        "from": ".",
        "to": ".",
        "filter": [
          "electron",
          "images",
          "node_modules",
          "main.js",
          "package.json",
          "gif"
        ]
      },
      {
        "from": "./build",
        "to": ".",
        "filter": ["**/*"]
    }
    ],
    "directories": {
      "buildResources": "assets"
    },
    "appId": "com.rpsview.app",
    "productName": "rpsview",
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "icon.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "extends": null,
    "dmg": {
      "window": {
        "width": 544,
        "height": 408
      },
      "contents": [
        {
          "x": 145,
          "y": 257
        },
        {
          "x": 400,
          "y": 255,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
    
  }
}
